from math import *

x = float(input("Введите число x"))
y = float(input("Введите число y"))
z = float(input("Введите число z"))
a = float(input("Граница интервала от"))
b = float(input("Гграница интервала до"))
k = 0

if (x < a) or (x > b):
    k += 1
    print('x = ', x, 'НЕ входит в интервал')
if (y < a) or (y > b):
    k += 1
    print('y = ', y, 'НЕ входит в интервал')
if (z < a) or (z > b):
    k += 1
    print('z = ', z , 'НЕ входит в интервал')
print (k, 'числа НЕ входят в заданный интервал (',a,';',b,')')


Введите число x
1
Введите число y
5
Введите число z
9
Граница интервала от
2
Гграница интервала до
8
x =  1.0 НЕ входит в интервал
z =  9.0 НЕ входит в интервал
2 числа НЕ входят в заданный интервал ( 2.0 ; 8.0 )
